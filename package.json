{
	"name": "@adexchange/aeg-redis",
	"version": "0.0.1",
	"description": "Redis client",
	"main": "lib/index.js",
	"author": "Ad Exchange Group",
	"repository": {
		"type": "git",
		"url": "https://github.com/AdExchangeGrp/aeg-redis.git"
	},
	"license": "UNLICENSED",
	"scripts": {
		"test": "mocha --recursive --compilers ts:ts-node/register test/**/*.ts",
		"test:coverage": "nyc npm run test",
		"lint": "tslint src/**/*.ts",
		"lint:watch": "npm run tslint && onchange \"**/*.ts\" \"tslint.json\" -- npm run lint",
		"compile": "tsc",
		"compile:watch": "npm run compile -- --watch",
		"dev": "npm run compile:watch",
		"build": "npm run lint && npm run compile && npm run build:coverage",
		"build:test": "mocha --recursive --compilers ts:ts-node/register --reporter mocha-junit-reporter test/unit/**/*.ts",
		"build:coverage": "nyc npm run build:test",
		"patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.8",
		"@types/lodash": "^4.14.68",
		"@types/mocha": "^2.2.41",
		"@types/node": "^6.0.0",
		"@types/redis": "^2.6.0",
		"istanbul": "^1.1.0-alpha.1",
		"mocha": "^3.2.0",
		"mocha-junit-reporter": "^1.13.0",
		"nyc": "^11.0.3",
		"onchange": "^3.2.1",
		"should": "^11.2.0",
		"ts-node": "^3.2.0",
		"tslint": "^5.5.0",
		"typescript": "^2.4.1"
	},
	"dependencies": {
		"bluebird": "^3.5.0",
		"lodash": "^4.17.4",
		"redis": "^2.7.1"
	},
	"engine": "node >= 6.10.0"
}
